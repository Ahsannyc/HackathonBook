{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/auth/onboarding/(.*)",
      "dest": "/auth/onboarding/index.html"
    },
    {
      "src": "/auth/profile/(.*)",
      "dest": "/auth/profile/index.html"
    },
    {
      "src": "/auth/signin/(.*)",
      "dest": "/auth/signin/index.html"
    },
    {
      "src": "/auth/signup/(.*)",
      "dest": "/auth/signup/index.html"
    },
    {
      "src": "/docs/(.*)",
      "dest": "/docs/index.html"
    },
    {
      "src": "/blog/(.*)",
      "dest": "/blog/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ]
}